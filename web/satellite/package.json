    "stylelint-config-standard-vue": "1.0.0",
    "stylelint-scss": "6.2.1",
    "typescript": "5.3.3",
    "vite": "5.4.11",
    "vite-plugin-compression": "0.5.1",
    "vite-plugin-vuetify": "2.0.1",
    "vitest": "1.2.2",
    "vitest-fetch-mock": "0.2.2",
    "vue-eslint-parser": "9.4.2",
    "webfontloader": "1.6.28"
